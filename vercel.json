{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/src/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/src/public/js/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ]
}
